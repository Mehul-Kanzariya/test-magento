<?xml version="1.0"?>
<schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Setup/Declaration/Schema/etc/schema.xsd">
  <table name="form_data" resource="default" engine="innodb" comment="About Your Table">
    <column xsi:type="int" name="entity_id" padding="11" unsigned="false" nullable="false" identity="true" comment="ID"/>
    <column xsi:type="text" name="username" nullable="false" comment="User Name"/>
    <column xsi:type="text" name="email" nullable="false" comment="User Email"/>
    <column xsi:type="text" name="telephone" nullable="false" comment="User Telephone"/>
    <constraint xsi:type="primary" referenceId="PRIMARY">
      <column name="entity_id"/>
    </constraint>
  </table>
</schema>
